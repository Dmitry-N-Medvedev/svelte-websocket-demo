{
  "name": "@dmitry-n-medvedev/fbs",
  "private": true,
  "version": "0.0.0",
  "description": "flatbuffers",
  "type": "module",
  "engines": {
    "node": "19.3.0",
    "pnpm": "7.19.0"
  },
  "engineStrict": true,
  "volta": {
    "node": "19.3.0"
  },
  "bin": "bin/compile.mjs",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "rename:js:mjs": "./node_modules/.bin/renamer --force --silent --find .js --replace .mjs **/compiled/mjs/**",
    "lint:mjs": "./node_modules/.bin/eslint ./ --ext .mjs --fix",
    "build:schema": "node ./bin/compile.mjs && pnpm run rename:js:mjs && pnpm run lint:mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dmitry-N-Medvedev/svelte-websocket-demo.git"
  },
  "author": "Dmitry N. Medvedev <dmitry.medvedev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dmitry-N-Medvedev/svelte-websocket-demo/issues"
  },
  "homepage": "https://github.com/Dmitry-N-Medvedev/svelte-websocket-demo#readme",
  "dependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "execa": "^6.1.0",
    "flatbuffers": "^22.12.6",
    "renamer": "^4.0.0"
  }
}

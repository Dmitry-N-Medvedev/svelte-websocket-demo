{
  "name": "@dmitry-n-medvedev/svelte-websocket-demo-front-end",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.4.0",
  "engines": {
    "node": "20.1.0",
    "pnpm": "8.4.0"
  },
  "engineStrict": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test-skip": "playwright test",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "dependencies": {
    "@dmitry-n-medvedev/common": "workspace:^0.0.0",
    "@dmitry-n-medvedev/deserializers.moneymessage": "workspace:^0.0.0",
    "@dmitry-n-medvedev/fbs": "workspace:^0.0.0",
    "@dmitry-n-medvedev/serializers.donatemessage": "workspace:^0.0.0",
    "flatbuffers": "^23.1.4",
    "inter-ui": "^3.19.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@sveltejs/adapter-node": "1.1.0",
    "@sveltejs/kit": "1.0.11",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.2",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.27.0"
  },
  "dependenciesMeta": {
    "@dmitry-n-medvedev/common": {
      "injected": true
    },
    "inter-ui": {
      "injected": true
    }
  }
}

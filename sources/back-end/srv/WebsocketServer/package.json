{
  "name": "@dmitry-n-medvedev/websocketserver",
  "version": "0.0.0",
  "description": "Websocket Server",
  "type": "module",
  "engines": {
    "node": "=19.2.0",
    "pnpm": "=7.18.1"
  },
  "engineStrict": true,
  "volta": {
    "node": "19.2.0"
  },
  "main": "./server.mjs",
  "bin": "./server.mjs",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:server": "./node_modules/.bin/linkemon -V ./server.mjs"
  },
  "author": "Dmitry N. Medvedev <dmitry.medvedev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@dmitry-n-medvedev/libwebsocketserver": "workspace:^0.0.0",
    "dotenv": "^16.0.1"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "colours": true,
    "ext": "mjs",
    "verbose": true,
    "exec": "./server.mjs",
    "watch": [
      "./node_modules/@dmitry-n-medvedev/**/*.mjs",
      "./server.mjs"
    ],
    "events": {
      "start": "clear"
    }
  },
  "devDependencies": {
    "linkemon": "^0.1.1",
    "nodemon": "^2.0.16"
  }
}
